
public class MyDate2 {
	private int day;
 	private int month;
 	private int year;
 	
	public MyDate2(int day, int month, int year) {
		if(day >= 1 && day <= 31) {
			this.day = day;
		} else {
			this.day = 1;
		}
		
		if(month >= 1 && month <= 12) {
			this.month = month;
		} else {
			this.month = 1;
		}
		
		this.month = month;
		this.year = year;
	}
 	
 	
}
