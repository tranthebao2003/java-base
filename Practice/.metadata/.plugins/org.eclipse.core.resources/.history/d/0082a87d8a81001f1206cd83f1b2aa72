
public class MyDate2 {
	private int day;
	private int month;
	private int year;

	public MyDate2(int day, int month, int year) {
		if (day >= 1 && day <= 31) {
			this.day = day;
		} else {
			this.day = 1;
		}

		if (month >= 1 && month <= 12) {
			this.month = month;
		} else {
			this.month = 1;
		}

		if (year >= 1) {
			this.year = year;
		} else {
			this.year = 1;
		}

	}

	public int getDay() {
		return day;
	}

	public void setDay(int day) {
		if (day >= 1 && day <= 31) {
			this.day = day;
		}
	}

	public int getMonth() {
		return month;
	}

	public void setMonth(int month) {
		if (month >= 1 && month <= 12)
			this.month = month;
	}

	public int getYear() {
		return year;
	}

	public void setYear(int year) {
		if (year >= 1)
		this.year = year;
	}

	public static void main(String[] args) {
		MyDate2 md = new MyDate2(31, 1, 2021);
		System.out.println("Day = " + md.getDay());
		md.setDay(0);
		md.setDay(20);
		System.out.println("Day = " + md.getDay());
		
		md.setMonth(0);
		md.setMonth(12);
		System.out.println("Day = " + md.getMonth());
	}

}
